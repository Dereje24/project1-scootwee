<div id='page'>
<button id='ok'>OK</button>
<h1>Users#show</h1>
<p>Find me in app/views/users/show.html.erb</p>
<%= link_to 'Logout' ,logout_path %>
<br>

<div id='list' style="height:500px;width:700px;float:left;margin-right:50px">

</div>
<div id='map' style="height:500px;width:400px;float:left"></div>
</div>
<div class="modal fade" id="myModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title">Confirm reservation</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
        <div class='info'></div>
      <%= form_with url: rental_path,id:'reservation_form' do |t| %>
      <%= t.hidden_field :local_id %>
      <%= t.hidden_field :vehicle_id %>

      </div>
      <div class="modal-footer">
        <button type="button" id="reserve" class="btn btn-primary">Reserve</button>
        <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
  <% end %>
      </div>
    </div>
  </div>
</div>
