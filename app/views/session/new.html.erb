<%= form_with mode:@user , id:'login' do |t|%>

<div class= 'everything'>
<div class= 'logsession'>

  <h1>Login</h1>
  
<div>
<%= t.label :email , 'Email' %>
<br>
<%= t.text_field :email , placeholder:'Email here' %>
</div>

<div>
<%= t.label :password, 'Password' %>
<br>
<%= t.password_field :password , placeholder:'Password' %>
</div>

<%= t.hidden_field :lat %>
<%= t.hidden_field :lng %>
<br>
<%= t.submit 'Login' %>
<% end %>
</div>

</div>




